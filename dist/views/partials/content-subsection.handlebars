<section class="main__sub-section" id="{{group}}-{{type}}">
  
  <h{{plus level 1}} class="main__heading--secondary">
    
    <div class="container">{{#if (eq type 'css') }}{{type}}{{else}}{{type}}s{{/if}}</div>
    
  </h{{plus level 1}}>
  
  {{#each items}}
  
    {{> item group=../group type=../type}}
  
  {{/each}}
  
</section>