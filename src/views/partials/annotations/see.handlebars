{{set 'titleDisplayed' false item}}
{{set 'rootDisplay' self.[@root].display item}}

{{#each item.see}}

  {{#if (and (gt (indexOf ../item.rootDisplay.access access) -1) (not (and ../item.rootDisplay.alias alias)))}}

    {{#if (not ../item.titleDisplayed)}}

      {{set 'titleDisplayed' true ../item}}

      <h3 class="item__sub-heading">See</h3>
      <ul class="list-unstyled">
        
    {{/if}}

    <li>
      <span class="item__cross-type">[{{context.type}}]</span> 
      <a href="#{{context.type}}-{{context.name}}">
        <code>{{context.name}}</code>
      </a>
    </li>
        
  {{/if}}

{{/each}}

{{#if item.titleDisplayed}}
  </ul>
{{/if}}

{{unset 'titleDisplayed' item}}
{{unset 'rootDisplay' item}}